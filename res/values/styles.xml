<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    
      <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    
    <style name="text" parent="@android:style/TextAppearance.Medium">  
    	 <item name="android:layout_width">fill_parent</item>  
        <item name="android:layout_height">wrap_content</item>  
    	 <item name="android:textColor">#ff000000</item>  
    	 <item name="android:textStyle">bold</item>  
    	 <item name="android:typeface">monospace</item>  
     	 <item name="android:maxLines">1</item>  
     	 <item name="android:textSize">10dp</item>  
   </style>    
   
    <style name="backgroud">  
    	 <item name="android:background">#333333</item>  
    </style>
    
    <style name="ButtonCicle">
        <item name="android:layout_width">120dp</item>  
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
    </style>
        
    <style name="ButtonCicle.BtnLike" >
        <item name="android:background">@drawable/like_buttons</item>
    </style>
    <style name="ButtonCicle.BtnUnLike" >  
        <item name="android:background">@drawable/unlike_buttons</item>
    </style>

</resources>
